{
  "name": "Quora Charm",
  "version": "0.1",
  "description": "Chrome extension for Quora.",
  "browser_action": {
    "default_icon": "images/icon_16.png",
    "default_title": "Quora Charm",
    "popup": "popup.html"
  },
  "permissions": [
    "*://*.quora.com/",
    "background",
    "contextMenus",
    "tabs"
  ],
   "background_page": "background.html",
  "icons": {
    "16": "images/icon_16.png",
    "48": "images/icon_48.png",
    "128": "images/icon_48.png"
  }
}

