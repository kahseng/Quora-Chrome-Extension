{
  "name": "Quora Charm",
  "version": "0.1",
  "description": "Chrome extension for Quora.",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "Quora Charm",
    "popup": "popup.html"
  },
  "permissions": [
    "http://api.quora.com/",
    "background",
    "contextMenus"
  ],
   "background_page": "background.html"
}

